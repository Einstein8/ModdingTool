<packer type="acc">
	<!-- This will be copied as manifest.xml -->
	<guid>modbuilder.example.accessory</guid>
	<name>MODBUILDER ACCESSORY EXAMPLE</name>
	<version>1.0.0</version>
	<author>hooh</author>
	<description>This is modbuilder example.</description>

	<!-- This section will contain bundle information -->
	<bundles>
		<bundle path="example/glasses.unity3d">
			<asset path="rayben.prefab"/>
		</bundle>
		<bundle path="example/glasses_thumb.unity3d">
			<asset path="thumb_image.psd"/>
		</bundle>
	</bundles>

	<!-- This section will contain build information -->
	<build name="@example_accessory">
		<list type="accglasses">
			<item
				kind="0" possess="1" name="@@EXAMPLE ACCESSORY" en_us="" 
				mesh-manifest="abdata" mesh-bundle="example/glasses.unity3d" mesh-a="rayben"
				parent="N_Megane" thumb-bundle="example/glasses_thumb.unity3d" thumb="thumb_image"
			/>
		</list>
	</build>
</packer>