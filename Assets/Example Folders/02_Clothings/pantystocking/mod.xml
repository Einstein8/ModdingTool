<packer type="fclothe">
	<!-- This will be copied as manifest.xml -->
	<guid>modbuilder.example.panst</guid>
	<name>MODBUILDER PANTYSTOCKING EXAMPLE</name>
	<version>1.0.0</version>
	<author>hooh</author>
	<description>This is modbuilder example.</description>

	<!-- This section will contain bundle information -->
	<bundles>
		<bundle path="example/pantystocking.unity3d">
			<asset path="clothmesh.prefab"/>
			<asset path="diffuse.psd"/>
			<asset path="colormask.png"/>
			<asset path="normal.png"/>
		</bundle>
		<bundle path="example/pantystocking_thumb.unity3d">
			<asset path="thumb_image.psd"/>
		</bundle>
	</bundles>

	<!-- This section will contain build information -->
	<build name="@example_pantystocking">
		<list type="fpanst">
			<item
				kind="0" possess="1" name="@@EXAMPLE PANTYSTOCKING" en_us="0" 
				mesh-manifest="abdata" mesh-bundle="example/pantystocking.unity3d" mesh-a="clothmesh" state="1"
            	tex-main="diffuse" tex-mask="colormask" tex-main2="0" tex-mask2="0" hide-bottom="0"
				thumb-bundle="example/pantystocking_thumb.unity3d" thumb="thumb_image"
			/>
		</list>
	</build>
</packer>