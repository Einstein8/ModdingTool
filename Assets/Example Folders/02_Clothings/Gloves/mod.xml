<packer type="fclothe">
	<!-- This will be copied as manifest.xml -->
	<guid>com.hooh.catglove</guid>
	<name>Cat Paws</name>
	<version>1.0.0</version>
	<author>hooh</author>
	<description>Test Mate.</description>

	<!-- This section will contain bundle information -->
	<bundles>
		<!-- An assetbundle contains mesh objects. -->
		<bundle path="hoohchar/catglove_mesh.unity3d">
			<asset path="shoes.prefab"/>
			<asset path="gloves.prefab"/>
			<asset path="diffuse.tga"/>
			<asset path="colormask.psd"/>
		</bundle>
		<!-- An assetbudnle contains thumbnail. -->
		<bundle path="hoohthumbs/catglove_thumb.unity3d">
			<asset path="thumb_image.png"/>
		</bundle>
	</bundles>

	<!-- This section will contain build information -->
	<build name="hooh_catpaws">
		<list type="fgloves">
			<item
				kind="0" possess="1" name="[hooh] Cat Gloves" en_us="" 
				mesh-manifest="abdata" mesh-bundle="hoohchar/catglove_mesh.unity3d" mesh-a="gloves"
				state="1"
            	tex-main="diffuse" tex-mask="colormask" tex-main2="0" tex-mask2="0" thumb="thumb_image"
			/>
		</list>
		<list type="fshoes">
			<item
				kind="0" possess="1" name="[hooh] Cat Shoes" en_us="" 
				mesh-manifest="abdata" mesh-bundle="hoohchar/catglove_mesh.unity3d" mesh-a="shoes"
				state="1"
            	tex-main="diffuse" tex-mask="colormask" tex-main2="0" tex-mask2="0" thumb="thumb_image"
			/>
		</list>
	</build>
</packer>