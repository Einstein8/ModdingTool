<packer type="fclothe">
	<!-- This will be copied as manifest.xml -->
	<guid>modbuilder.example.suit</guid>
	<name>MODBUILDER SUIT EXAMPLE</name>
	<version>1.0.0</version>
	<author>hooh</author>
	<description>This is modbuilder example.</description>

	<!-- This section will contain bundle information -->
	<bundles>
		<bundle path="example/babydoll.unity3d">
			<asset path="clothmesh.prefab"/>
			<asset path="tempmat.mat"/>
			<asset path="diffuse.psd"/>
			<asset path="colormask.png"/>
		</bundle>
		<bundle path="example/babydoll_thumb.unity3d">
			<asset path="thumb_image.psd"/>
		</bundle>
	</bundles>
	<!-- This section will contain build information -->
	<build name="@example_babydoll">
		<list type="ftop">
			<item
				kind="0" possess="1" name="[hooh] clothmesh Test" en_us="0" state="1" coordinate="1"
				mesh-manifest="abdata" mesh-bundle="example/babydoll.unity3d" mesh-a="clothmesh"
				no-bra="0" bodymask-bundle="0" bodymask-tex="0" bramask-bundle="0" bramask-tex="0" breakmask-tex="0"
				innermask-tb-bundle="0" innermask-tb-tex="0" innermask-b-bundle="0" innermask-b-tex="0"
				panstmask-bundle="0" panstmask-tex="0" bodymask-b-bundle="0" bodymask-b-tex="0"
				tex-main="diffuse" tex-mask="colormask" tex-main2="0" tex-mask2="0" tex-main3="0" tex-mask3="0" hide-bottom="0"
				thumb-bundle="example/babydoll_thumb.unity3d" thumb="thumb_image"
			/>
		</list>
	</build>
</packer>